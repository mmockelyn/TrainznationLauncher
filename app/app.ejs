<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Trainznation Launcher</title>
    <link rel="stylesheet" href="../node_modules/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="../node_modules/material-icons/iconfont/material-icons.css">
    <link rel="stylesheet" href="./assets/css/launcher-base.css">
</head>
<body>
    <% include frame.ejs %>
    <div class="card mb-4 ml-4 bg-dark" style="max-width: 95%;">
        <div class="row no-gutters">
            <div class="col-4">
                <img src="./assets/images/routes/pdl.png" class="card-img">
            </div>
            <div class="col-8">
                <div class="card-body">
                    <div class="row">
                        <div class="col-10">
                            <h5 class="card-title">Pays de la Loire</h5>
                            <p id="pdlVersionText" class="card-text"></p>
                            <p class="card-text"><small id="pdlVersionState" class="text-muted"></small></p>
                        </div>
                        <div class="col-2">
                            <button id="btnLaunch" class="btn btn-secondary" style="position: relative; top: 45px;">
                                <span class="material-icons" style="position: relative; top: 5px;">build</span>
                            </button>
                            <button id="btnTest" class="btn btn-secondary" style="position: relative; top: 45px;">
                                    <span class="material-icons" style="position: relative; top: 5px;">build</span>
                                </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>require('popper.js')</script>
    <script>require('bootstrap')</script>
    <script src="./assets/js/app.js"></script>
</body>
</html>