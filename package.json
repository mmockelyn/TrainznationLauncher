    "electron-store": "^5.0.0",
    "electron-updater": "^4.2.0",
    "jquery": "^3.4.1",
    "material-icons": "^0.3.1",
    "node-powershell": "^4.0.0",
    "popper.js": "^1.16.0",
    "sweetalert2": "^8.18.6"
  },
  "devDependencies": {
    "electron": "^7.0.0",
    "cross-env": "^6.0.3",
    "electron-builder": "^22.1.0",
    "eslint": "^6.6.0"
